
<form action="/admin/product/add" method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <label for="exampleInputEmail1">Name</label>
        <input type="name" value="" name="name" value="" class="form-control" id="exampleInputEmail1" >
    </div>
    <input type="hidden" name="idds" value="<%= cateAlias %>">
    <div class="form-group">
        <label for="exampleInputCate">Thương hiệu</label>                                
        <select class="form-control" name="nsx" id="exampleInputCate">
            <% thuonghieu.forEach(e => { %>
               
                <option value= '<%= e.id %>'> <%= e.ten_nsx %></option>
               
            <% }) %>
        </select>
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Hình ảnh</label>
        
        <input type="file" name="file" id="exampleInputImg" >
        <input type="file" name="file"  id="exampleInputImg" >
        <input type="file" name="file"  id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Giá (VND)</label>
        <input type="name" name="price" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Sale Off (VND)</label>
            <input type="name" name="saleOff" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Số lượng</label>
        <input type="name" name="quality" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Bảo hành</label>
            <input type="name" name="baohanh" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Màu sắc</label>
            <input type="name" name="mausac" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Độ tương phản</label>
            <input type="name" name="dotuongphan" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Kích thước</label>
            <input type="name" name="kichthuoc" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
            <label for="exampleInputImg">Độ sáng</label>
            <input type="name" name="dosang" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Độ phân giải</label>
        <input type="name" name="dophangiai" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Cổng kết nối</label>
        <input type="name" name="congketnoi" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Phản hồi</label>
        <input type="name" name="phanhoi" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Tần số quét</label>
        <input type="name" name="tansoquet" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Tỷ lệ</label>
        <input type="name" name="tyle" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Góc nhìn</label>
        <input type="name" name="gocnhin" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Tấm nền</label>
        <input type="name" name="tamnen" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="exampleInputImg">Công nghệ màn hình</label>
        <input type="name" name="congnghe" value=""  class="form-control" id="exampleInputImg" >
    </div>
    <div class="form-group">
        <label for="description">Chi tiết</label>
        <textarea id="description" name="description"  class="form-control" rows="4" ></textarea>
        
        
    </div>
    
    
    <button type="submit" class="btn btn-primary">Save</button>
    <button class="btn btn-danger">Cancel</button>
</form>
